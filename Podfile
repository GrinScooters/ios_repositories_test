# Uncomment the next line to define a global platform for your project
 platform :ios, '10.0'

target 'iCloud key' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iCloud key

  pod 'AsyncRequest', :git => 'https://github.com/GrinScooters/grin_ios_async_request.git'
  pod 'Alamofire'  

end
